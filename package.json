{
  "license": "MIT",
  "name": "eleventy-webpack",
  "repository": {
    "type": "git",
    "url": "https://github.com/clenemt/eleventy-webpack"
  },
  "scripts": {
    "clean": "rimraf _site",
    "format": "prettier --write **/*.{js,s?css,yml} !_site",
    "start": "cross-env NODE_ENV=development npm-run-all -l clean --parallel start:*",
    "start:webpack": "webpack --watch",
    "start:eleventy": "eleventy --serve",
    "build": "cross-env NODE_ENV=production npm-run-all -l clean build:*",
    "build:webpack": "webpack",
    "build:eleventy": "eleventy",
    "analyze": "npm-run-all build analyze:*",
    "analyze:js": "source-map-explorer _site/**/*.js _site/**/*.js.map"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-img": "^0.9.0",
    "@11ty/eleventy-navigation": "^0.2.0",
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "babel-loader": "^8.2.2",
    "cheerio": "^1.0.0-rc.10",
    "core-js": "^3.14.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.1",
    "date-fns": "^2.22.1",
    "eleventy-plugin-error-overlay": "^0.1.2",
    "html-minifier": "^4.0.0",
    "js-yaml": "^3.14.1",
    "markdown-it": "^12.0.6",
    "markdown-it-anchor": "^7.1.0",
    "markdown-it-attrs": "^4.0.0",
    "markdown-it-bracketed-spans": "^1.0.1",
    "mini-css-extract-plugin": "^1.6.0",
    "npm-run-all": "^4.1.5",
    "outdent": "^0.8.0",
    "postcss": "^8.3.3",
    "postcss-loader": "^6.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.1",
    "rimraf": "^3.0.2",
    "sass": "^1.34.1",
    "sass-loader": "^12.1.0",
    "source-map-explorer": "^2.5.2",
    "svg-sprite": "^1.5.0",
    "tailwindcss": "^2.1.4",
    "vinyl": "^2.2.1",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2",
    "webpack-manifest-plugin": "^3.1.1"
  }
}
