{
  "license": "MIT",
  "name": "eleventy-webpack",
  "repository": {
    "type": "git",
    "url": "https://github.com/clenemt/eleventy-webpack"
  },
  "scripts": {
    "format": "prettier --write **/*.{js,s?css,yml} !_site",
    "start": "cross-env NODE_ENV=development run-p -l start:*",
    "start:webpack": "webpack --config webpack.dev.js ",
    "start:eleventy": "eleventy --serve",
    "build": "cross-env NODE_ENV=production run-s -l build:*",
    "build:clean": "rimraf _site",
    "build:webpack": "webpack --config webpack.prod.js",
    "build:eleventy": "eleventy",
    "analyze": "run-s build analyze:*",
    "analyze:js": "source-map-explorer _site/**/*.js _site/**/*.js.map"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-es2015-modules-commonjs-simple": "^6.7.4",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "date-fns": "^2.16.1",
    "eleventy-plugin-error-overlay": "^0.1.2",
    "eleventy-plugin-local-images": "^0.4.0",
    "file-loader": "^6.2.0",
    "html-minifier": "^4.0.0",
    "html-webpack-plugin": "^4.5.0",
    "js-yaml": "^3.14.0",
    "markdown-it": "^12.0.2",
    "markdown-it-anchor": "^6.0.0",
    "markdown-it-attrs": "^3.0.3",
    "mini-css-extract-plugin": "^1.2.1",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.1.6",
    "postcss-loader": "^4.0.4",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.0.5",
    "source-map-explorer": "^2.5.0",
    "svg-sprite": "^1.5.0",
    "vinyl": "^2.2.1",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-merge": "^5.3.0"
  }
}
